{
  "expectation_suite_name": "notify-login_events_suite",
  "expectations": [
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "id",
        "regex": "^[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}$"
      },
      "meta": {
        "description": "id should match UUID format"
      }
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "user_id",
        "regex": "^[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}$"
      },
      "meta": {
        "description": "user_id should match UUID format"
      }
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "data",
        "regex": ".*"
      },
      "meta": {
        "description": "data should be string-like"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_type_list",
      "kwargs": {
        "column": "created_at",
        "type_list": [
          "datetime64",
          "datetime64[ns]",
          "<M8[us]"
        ]
      },
      "meta": {}
    },
    {
      "expectation_type": "expect_column_values_to_be_in_type_list",
      "kwargs": {
        "column": "updated_at",
        "type_list": [
          "datetime64",
          "datetime64[ns]",
          "<M8[us]"
        ]
      },
      "meta": {}
    },
    {
      "expectation_type": "expect_column_values_to_be_in_type_list",
      "kwargs": {
        "column": "data_modified",
        "type_list": [
          "datetime64",
          "datetime64[ns]",
          "<M8[us]"
        ]
      },
      "meta": {}
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "year",
        "regex": "^\\d{4}$"
      },
      "meta": {
        "description": "year should be a 4-digit year (e.g., 2024)"
      }
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "month",
        "regex": "^\\d{4}-\\d{2}$"
      },
      "meta": {
        "description": "month should be in the format yyyy-mm (e.g., 2024-06)"
      }
    }
  ],
  "meta": {
    "great_expectations_version": "0.18.22"
  }
}