{
  "expectation_suite_name": "notify-user_to_organisation_suite",
  "expectations": [
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "user_id",
        "regex": "^[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}$"
      },
      "meta": {
        "description": "user_id should match UUID format"
      }
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "organisation_id",
        "regex": "^[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}$"
      },
      "meta": {
        "description": "organisation_id should match UUID format"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_type_list",
      "kwargs": {
        "column": "data_modified",
        "type_list": [
          "datetime64",
          "datetime64[ns]",
          "<M8[us]"
        ]
      },
      "meta": {}
    }
  ],
  "meta": {
    "great_expectations_version": "0.18.22"
  }
}