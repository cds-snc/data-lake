{
  "expectation_suite_name": "notify-users_suite",
  "expectations": [
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "id",
        "regex": "^[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}$"
      },
      "meta": {
        "description": "id should match UUID format"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_type_list",
      "kwargs": {
        "column": "created_at",
        "type_list": [
          "datetime64",
          "datetime64[ns]",
          "<M8[us]",
          "TimestampType",
          "timestamp"
        ]
      },
      "meta": {}
    },
    {
      "expectation_type": "expect_column_values_to_be_in_type_list",
      "kwargs": {
        "column": "updated_at",
        "type_list": [
          "datetime64",
          "datetime64[ns]",
          "<M8[us]",
          "TimestampType",
          "timestamp"
        ]
      },
      "meta": {}
    },
    {
      "expectation_type": "expect_column_values_to_be_in_type_list",
      "kwargs": {
        "column": "password_changed_at",
        "type_list": [
          "datetime64",
          "datetime64[ns]",
          "<M8[us]",
          "TimestampType",
          "timestamp"
        ]
      },
      "meta": {}
    },
    {
      "expectation_type": "expect_column_values_to_be_in_type_list",
      "kwargs": {
        "column": "logged_in_at",
        "type_list": [
          "datetime64",
          "datetime64[ns]",
          "<M8[us]",
          "TimestampType",
          "timestamp"
        ]
      },
      "meta": {}
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "failed_login_count",
        "min_value": 0
      },
      "meta": {}
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "state",
        "regex": ".*"
      },
      "meta": {
        "description": "state should be string-like"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "platform_admin",
        "value_set": [
          true,
          false
        ]
      },
      "meta": {
        "description": "platform_admin should be boolean-like values"
      }
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "auth_type",
        "regex": ".*"
      },
      "meta": {
        "description": "auth_type should be string-like"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "blocked",
        "value_set": [
          true,
          false
        ]
      },
      "meta": {
        "description": "blocked should be boolean-like values"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "password_expired",
        "value_set": [
          true,
          false
        ]
      },
      "meta": {
        "description": "password_expired should be boolean-like values"
      }
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "year",
        "regex": "^\\d{4}$"
      },
      "meta": {
        "description": "year should be a 4-digit year (e.g., 2024)"
      }
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "month",
        "regex": "^\\d{4}-\\d{2}$"
      },
      "meta": {
        "description": "month should be in the format yyyy-mm (e.g., 2024-06)"
      }
    }
  ],
  "meta": {
    "great_expectations_version": "0.18.22"
  }
}