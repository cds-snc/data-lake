{
  "name": "data-lake",
  "image": "mcr.microsoft.com/devcontainers/base:bullseye@sha256:fb17cc95abb5489d81d129c8e5f58c7c1985d009397f2718a7d1d4c26f94b302",
  "containerEnv": {
    "SHELL": "/bin/zsh"
  },
  "features": {
    "ghcr.io/devcontainers/features/aws-cli:1": {
      "version": "latest"
    },
    "ghcr.io/devcontainers/features/python:1": {
      "version": "3.14"
    },
    "ghcr.io/devcontainers/features/terraform:1": {
      "version": "1.14.4",
      "terragrunt": "0.68.6"
    }
  },
  "postCreateCommand": "sudo apt-get update && sudo apt-get install -y openjdk-17-jdk && export JAVA_HOME=/usr/lib/jvm/java-17-openjdk-amd64",
  "customizations": {
    "vscode": {
      "extensions": [
        "hashicorp.terraform",
        "github.copilot",
        "github.vscode-github-actions",
        "hashicorp.hcl",
        "redhat.vscode-yaml"
      ]
    }
  },
  "remoteUser": "vscode"
}